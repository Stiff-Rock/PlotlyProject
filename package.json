{
  "name": "plotlyproject",
  "version": "1.0.0",
  "description": "A data visualization application made with plotly and electron",
  "main": "app/static/main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Stiff-Rock/PlotlyProject.git"
  },
  "author": "Stiff-Rock, D4vsus",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Stiff-Rock/PlotlyProject/issues"
  },
  "homepage": "https://github.com/Stiff-Rock/PlotlyProject#readme",
  "keywords": [
    "data-visualization",
    "visualization",
    "plotly",
    "electron",
    "machine-learning",
    "data-analysis"
  ],
  "devDependencies": {
    "@electron-forge/cli": "^7.6.0",
    "@electron-forge/maker-deb": "^7.6.0",
    "@electron-forge/maker-rpm": "^7.6.0",
    "@electron-forge/maker-squirrel": "^7.6.0",
    "@electron-forge/maker-zip": "^7.6.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.6.0",
    "@electron-forge/plugin-fuses": "^7.6.0",
    "@electron/fuses": "^1.8.0",
    "electron": "^33.2.1"
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.1"
  },
  "config": {
    "forge": {
      "packagerConfig": {
        "extraFiles": [        {
          "from": "plotly_project",
          "to": "plotly_project", 
          "filter": ["**/*"]
        },
          {
            "from": ".",
            "to": ".",
            "filter": [
              "**/*"
            ]
          }
        ]
      },
      "makers": [
        {
          "name": "@electron-forge/maker-dmg",
          "config": {
            "icon": "path-to-your-app-icon"
          }
        }
      ]
    }
  }
}
